		serial1 = "/smb/motherboard/iofpga@7,00000000/uart@0a000";
		serial2 = "/smb/motherboard/iofpga@7,00000000/uart@0b000";
		serial3 = "/smb/motherboard/iofpga@7,00000000/uart@0c000";
