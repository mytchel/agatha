ETHERNET_LAN9188_OUT = arm/drivers/ethernet-lan9118/out.elf

ETHERNET_LAN9188_SRC_C = \
	arm/drivers/ethernet-lan9118/main.c

ETHERNET_LAN9188_LIBS = \
	lib/log \
	$(USER_LIBS)

ETHERNET_LAN9188_OBJECTS = \
	$(ETHERNET_LAN9188_SRC_C:%.c=%.o) \
	$(ETHERNET_LAN9188_LIBS:%=%/lib.a)

$(ETHERNET_LAN9188_OUT): $(ETHERNET_LAN9188_OBJECTS) 
	@echo LD $@ $(OBJECTS)
	@$(LD) $(LDFLAGS_USER) \
		-o $@ \
		$(ETHERNET_LAN9188_OBJECTS) \
		-lgcc

CLEAN += $(ETHERNET_LAN9188_OBJECTS)
CLEAN += $(ETHERNET_LAN9188_OUT)

