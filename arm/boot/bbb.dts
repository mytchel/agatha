		clocks = <0x4f>;
		enable-gpios = <0x4e 0x1b 0x0>;
		linux,phandle = <0x52>;
		phandle = <0x52>;
	};

	sound {
		compatible = "simple-audio-card";
		simple-audio-card,name = "TI BeagleBone Black";
		simple-audio-card,format = "i2s";
		simple-audio-card,bitclock-master = <0x50>;
		simple-audio-card,frame-master = <0x50>;

		simple-audio-card,cpu {
			sound-dai = <0x51>;
			clocks = <0x52>;
			linux,phandle = <0x50>;
			phandle = <0x50>;
		};

		simple-audio-card,codec {
			sound-dai = <0x53>;
		};
	};
};
